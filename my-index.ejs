    <link rel="shortcut icon" type="image/x-icon" href="./static/favicon.png" />
    <meta charset="UTF-8">
    <meta name="description" content="<%= htmlWebpackPlugin.options.app.name %>">
    <meta name="author" content="<%= htmlWebpackPlugin.options.author.name %> | <%= htmlWebpackPlugin.options.author.email %>">
